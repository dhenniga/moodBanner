<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.moodbanner.dev.moodbanner">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CreateNew" />
        <activity android:name=".EditExisting" />
        <activity android:name=".Gallery" />
        <activity android:name=".Menubar" />
        <activity
            android:name=".CreateStage"
            android:label="@string/title_activity_create_stage"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
